

```
create table Products (
	id bigint primary key ,
	name varchar not null ,
	price float
)
```


```
create table pickup_point (
	id bigint primary key ,
	points TEXT ,
	delivery DATE 
)
```


```
create table basket (
	id bigint primary key ,
	product_id INT
)
```






```
create table users (
	id bigint primary key ,
	login varchar ,
	passvord TEXT,
	email varchar 
)
```


```
create table orders (
	id bigint primary key ,
	product TEXT ,
	price float,
	pickup_point_id INT 
)
```




```
create table profile (
	id bigint primary key ,
	users_id INT ,
	basket_id INT,
	orders_id INT 
)
```



```
create table checks (
	id bigint primary key ,
	Sum_of_order float,
	order_id INT,
	product_id INT
)
```

