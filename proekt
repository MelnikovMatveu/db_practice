

```
create table Products (
	id bigint primary key ,
	name varchar not null ,
	price float
)
```


```
create table pickup_point (
	id bigint primary key ,
	points TEXT ,
	delivery DATE 
)
```


```
create table basket (
	id bigint primary key ,
	product_id INT
)
```


```
create table users (
	id bigint primary key ,
	login varchar ,
	passvord TEXT,
	email varchar 
)
```


```
create table orderd (
	id bigint primary key ,
	product TEXT ,
	price float,
	pickup_point_id INT 
)
```


